{
  "name": "nextjs-components",
  "version": "0.7.0",
  "author": {
    "name": "Kevin Wang",
    "url": "https://thekevinwang.com"
  },
  "description": "React components transcribed from the Vercel design system.",
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "emit-types": "npx tsc -p tsconfig.build.json",
    "build:babel": "babel src --extensions '.ts','.tsx' --copy-files --no-copy-ignored -d dist --ignore '**/*.test.*' --out-file-extension '.mjs' && npm run emit-types",
    "prune-dist": "find dist -type f -name '*.test.*' -delete",
    "clean": "rm -rf dist/",
    "prepublishOnly": "npm run clean && npm run build:babel && npm run prune-dist",
    "publish:local": "npx yalc publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nextjs-components/nextjs-components.git"
  },
  "keywords": [],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nextjs-components/nextjs-components/issues"
  },
  "homepage": "https://github.com/nextjs-components/nextjs-components#readme",
  "dependencies": {
    "@popperjs/core": "^2.11.5",
    "@reach/portal": "^0.17.0",
    "@react-aria/button": "^3.5.1",
    "@react-aria/dialog": "^3.2.1",
    "@react-aria/focus": "^3.6.1",
    "@react-aria/interactions": "^3.9.1",
    "@react-aria/overlays": "^3.9.1",
    "@react-aria/radio": "^3.2.1",
    "@react-aria/ssr": "^3.2.0",
    "@react-stately/overlays": "^3.3.1",
    "@react-stately/radio": "^3.4.1",
    "clsx": "^1.2.1",
    "react-is": "^18.2.0",
    "react-live": "^2.4.1",
    "react-merge-refs": "^2.0.1",
    "react-popper": "^2.3.0",
    "react-use-measure": "^2.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.2.5",
    "@types/jest": "^28.1.6",
    "@types/react": "^18.0.15",
    "@types/react-is": "^17.0.3",
    "jest": "^28.1.3",
    "jest-css-modules-transform": "^4.4.2",
    "jest-environment-jsdom": "^28.1.3",
    "next": "^12.2.2",
    "source-map-support": "^0.5.21",
    "ts-jest": "^28.0.6",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "next": ">=10.x",
    "react": "^18.2.0",
    "styled-jsx": "^5.0.2"
  }
}
