{
  "presets": [
    [
      "next/babel",
      {
        "preset-env": {
          "modules": false
        },
        "transform-runtime": {},
        "styled-jsx": {}
      }
    ]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "@components": "./src/components",
          "@contexts": "./src/contexts",
          "@hooks": "./src/hooks",
          "@icons": "./src/icons",
          "@styles": "./src/styles",
          "@utils": "./src/utils"
        }
      }
    ]
    // ["styled-jsx/babel", { "optimizeForSpeed": true }]
  ]
}
